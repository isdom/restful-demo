<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
        
    <bean class="org.jocean.restfuldemo.bll.DemoFlow"
          scope="prototype" />
    
    <bean id="ua2uri" class="java.util.concurrent.ConcurrentHashMap"/>
    
    <bean class="org.jocean.restfuldemo.bll.JumpFlow"
          scope="prototype">
          <property name="defaultRedirecturi" value="${redirecturi.default}"/>
          <property name="ua2uri" ref="ua2uri"/>
    </bean>
          
</beans>
