<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
        
    <bean class="org.jocean.restfuldemo.flow.DemoFlow"
          scope="prototype" />
    
    <bean class="org.jocean.restfuldemo.flow.JumpFlow"
          scope="prototype">
          <property name="wechatRedirecturi" value="${wechat.redirecturi}"/>
          <property name="alipayRedirecturi" value="${alipay.redirecturi}"/>
          <property name="qqRedirecturi" value="${qq.redirecturi}"/>
    </bean>
          
</beans>
